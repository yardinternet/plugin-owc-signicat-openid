{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "owc-signicat-openid/openid",
	"version": "0.0.1",
	"title": "OpenID",
	"description": "Show the OpenID login block",
	"attributes": {
		"redirectUrl": {
			"type": "string"
		},
		"idp": {
			"type": "string"
		},
		"buttonText": {
			"type": "string",
			"default": "Inloggen"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "owc-signicat-openid",
	"editorScript": "file:./openid.js",
	"editorStyle": "file:./openid.css",
	"style": "file:./style-openid.css"
}
